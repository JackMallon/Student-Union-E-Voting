<!doctype html>
<html class="fixed">
<head>

    <!-- Basic -->
    <meta charset="UTF-8">

    <title>Dashboard</title>
    <meta name="keywords" content="HTML5 Admin Template" />
    <meta name="description" content="JSOFT Admin - Responsive HTML5 Template">
    <meta name="author" content="JSOFT.net">
    <link rel="shortcut icon" href="/favicon.ico" type="image/x-icon"/>


    <!-- Mobile Metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />

    <!-- Web Fonts  -->
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700,800|Shadows+Into+Light" rel="stylesheet" type="text/css">

    <!-- Vendor CSS -->
    <link rel="stylesheet" href="../dashboard/vendor/bootstrap/css/bootstrap.css" />
    <link rel="stylesheet" href="../dashboard/vendor/font-awesome/css/font-awesome.css" />
    <link rel="stylesheet" href="../dashboard/vendor/magnific-popup/magnific-popup.css" />
    <link rel="stylesheet" href="../dashboard/vendor/bootstrap-datepicker/css/datepicker3.css" />

    <!-- Specific Page Vendor CSS -->
    <link rel="stylesheet" href="../dashboard/vendor/jquery-ui/css/ui-lightness/jquery-ui-1.10.4.custom.css" />
    <link rel="stylesheet" href="../dashboard/vendor/bootstrap-multiselect/bootstrap-multiselect.css" />
    <link rel="stylesheet" href="../dashboard/vendor/morris/morris.css" />

    <!-- Theme CSS -->
    <link rel="stylesheet" href="../dashboard/stylesheets/theme.css" />

    <!-- Skin CSS -->
    <link rel="stylesheet" href="../dashboard/stylesheets/skins/default.css" />

    <!-- Theme Custom CSS -->
    <link rel="stylesheet" href="../dashboard/stylesheets/theme-custom.css">

    <!-- Head Libs -->
    <script src="../dashboard/vendor/modernizr/modernizr.js"></script>

    <script src="http://code.jquery.com/jquery-1.11.1.min.js"></script>


</head>
<body>

    <section class="body">

        <!-- start: header -->
        <header class="header">
            <div class="logo-container">
                <a href="../" class="logo">
                    <img src="../dashboard/images/logo.png" height="35" alt="JSOFT Admin" />
                </a>
                <div class="visible-xs toggle-sidebar-left" data-toggle-class="sidebar-left-opened" data-target="html" data-fire-event="sidebar-left-opened">
                    <i class="fa fa-bars" aria-label="Toggle sidebar"></i>
                </div>
            </div>

            <!-- start: search & user box -->
            <div class="header-right">

                <span style="visibility: hidden" class="separator"></span>

                <div id="userbox" class="userbox">
                    <a href="#" data-toggle="dropdown">
                        <figure class="profile-picture">
                            <img src="../dashboard/images/!logged-user.jpg" alt="Joseph Doe" class="img-circle" data-lock-picture="assets/images/!logged-user.jpg" />
                        </figure>
                        <div class="profile-info" data-lock-name="John Doe" data-lock-email="johndoe@JSOFT.com">
                            <span class="name">{{ username }}</span>
                            <span style="color: black" class="role">student</span>
                        </div>

                        <i class="fa custom-caret"></i>
                    </a>

                    <div class="dropdown-menu">
                        <ul class="list-unstyled">
                            <li class="divider"></li>
                            <li>
                                <a role="menuitem" tabindex="-1" href="../logout"><i class="fa fa-power-off"></i> Logout</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
            <!-- end: search & user box -->
        </header>
        <!-- end: header -->
        <div class="inner-wrapper">
            <!-- start: sidebar -->
            <aside id="sidebar-left" class="sidebar-left">

                <div class="sidebar-header">
                    <div class="sidebar-title">
                        Navigation
                    </div>
                    <div class="sidebar-toggle hidden-xs" data-toggle-class="sidebar-left-collapsed" data-target="html" data-fire-event="sidebar-left-toggle">
                        <i class="fa fa-bars" aria-label="Toggle sidebar"></i>
                    </div>
                </div>

                <div class="nano">
                    <div class="nano-content">
                        <nav id="menu" class="nav-main" role="navigation">
                            <hr class="separator" />
                            {% block nav %}{% endblock %}
                        </nav>
                    </div>
                </div>

            </aside>

            {% block body %}{% endblock %}

            <!-- Vendor -->
            <script src="../dashboard/vendor/jquery/jquery.js"></script>
            <script src="../dashboard/vendor/jquery-browser-mobile/jquery.browser.mobile.js"></script>
            <script src="../dashboard/vendor/bootstrap/js/bootstrap.js"></script>
            <script src="../dashboard/vendor/nanoscroller/nanoscroller.js"></script>
            <script src="../dashboard/vendor/bootstrap-datepicker/js/bootstrap-datepicker.js"></script>
            <script src="../dashboard/vendor/magnific-popup/magnific-popup.js"></script>
            <script src="../dashboard/vendor/jquery-placeholder/jquery.placeholder.js"></script>

            <!-- Specific Page Vendor -->
            <script src="../dashboard/vendor/jquery-ui/js/jquery-ui-1.10.4.custom.js"></script>
            <script src="../dashboard/vendor/jquery-ui-touch-punch/jquery.ui.touch-punch.js"></script>
            <script src="../dashboard/vendor/jquery-appear/jquery.appear.js"></script>
            <script src="../dashboard/vendor/bootstrap-multiselect/bootstrap-multiselect.js"></script>
            <script src="../dashboard/vendor/jquery-easypiechart/jquery.easypiechart.js"></script>
            <script src="../dashboard/vendor/flot/jquery.flot.js"></script>
            <script src="../dashboard/vendor/flot-tooltip/jquery.flot.tooltip.js"></script>
            <script src="../dashboard/vendor/flot/jquery.flot.pie.js"></script>
            <script src="../dashboard/vendor/flot/jquery.flot.categories.js"></script>
            <script src="../dashboard/vendor/flot/jquery.flot.resize.js"></script>
            <script src="../dashboard/vendor/jquery-sparkline/jquery.sparkline.js"></script>
            <script src="../dashboard/vendor/raphael/raphael.js"></script>
            <script src="../dashboard/vendor/morris/morris.js"></script>
            <script src="../dashboard/vendor/gauge/gauge.js"></script>
            <script src="../dashboard/vendor/snap-svg/snap.svg.js"></script>
            <script src="../dashboard/vendor/liquid-meter/liquid.meter.js"></script>
            <script src="../dashboard/vendor/jqvmap/jquery.vmap.js"></script>
            <script src="../dashboard/vendor/jqvmap/data/jquery.vmap.sampledata.js"></script>
            <script src="../dashboard/vendor/jqvmap/maps/jquery.vmap.world.js"></script>
            <script src="../dashboard/vendor/jqvmap/maps/continents/jquery.vmap.africa.js"></script>
            <script src="../dashboard/vendor/jqvmap/maps/continents/jquery.vmap.asia.js"></script>
            <script src="../dashboard/vendor/jqvmap/maps/continents/jquery.vmap.australia.js"></script>
            <script src="../dashboard/vendor/jqvmap/maps/continents/jquery.vmap.europe.js"></script>
            <script src="../dashboard/vendor/jqvmap/maps/continents/jquery.vmap.north-america.js"></script>
            <script src="../dashboard/vendor/jqvmap/maps/continents/jquery.vmap.south-america.js"></script>

            <!-- Theme Base, Components and Settings -->
            <script src="../dashboard/javascripts/theme.js"></script>

            <!-- Theme Custom -->
            <script src="../dashboard/javascripts/theme.custom.js"></script>

            <!-- Theme Initialization Files -->
            <script src="../dashboard/javascripts/theme.init.js"></script>

        </div>
    </section>
</body>
</html>